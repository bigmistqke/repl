{
  "name": "@bigmistqke/repl",
  "version": "0.2.2",
  "description": "Virtual FileSystem and utilties for composing REPLs.",
  "license": "MIT",
  "author": "bigmistqke",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigmistqke/repl.git"
  },
  "homepage": "https://github.com/bigmistqke/repl#readme",
  "bugs": {
    "url": "https://github.com/bigmistqke/repl/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite serve dev",
    "serve:dev": "vite preview dev",
    "build:dev": "vite build dev",
    "build:dev-lib": "vite build --config vite.config.demo.ts",
    "build": "vite build .",
    "optimize": "vite optimize",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "@solidjs/router": "^0.15.2"
  },
  "peerDependencies": {
    "@babel/standalone": "^7.26.6",
    "dom-serializer": "^2.0.0",
    "domutils": "^3.2.2",
    "htmlparser2": "^10.0.0",
    "solid-js": "^1.6.0",
    "typescript": "^5.1.6",
    "monaco-editor": "^0.52.2"
  },
  "devDependencies": {
    "@bigmistqke/solid-grid-split": "^0.0.2",
    "@bigmistqke/vite-plugin-worker-proxy": "^0.0.12",
    "@bigmistqke/worker-proxy": "^0.0.12",
    "@monaco-editor/loader": "^1.4.0",
    "@types/babel__standalone": "^7.1.9",
    "concurrently": "^8.2.0",
    "prettier": "3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-dts-bundle-generator": "^2.0.4",
    "vite-plugin-solid": "^2.7.0",
    "vite-plugin-wasm": "^3.3.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^0.33.0",
    "wabt": "^1.0.36"
  },
  "keywords": [
    "solid",
    "repl",
    "playground",
    "typescript",
    "monaco"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.0"
  },
  "packageManager": "pnpm@9.1.1"
}
