{
  "name": "@bigmistqke/repl",
  "version": "0.1.4",
  "description": "Repl with typescript support, powered by solid-js and monaco-editor.",
  "license": "MIT",
  "author": "bigmistqke",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigmistqke/repl.git"
  },
  "homepage": "https://github.com/bigmistqke/repl#readme",
  "bugs": {
    "url": "https://github.com/bigmistqke/repl/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index/index.js",
  "module": "./dist/index/index.js",
  "types": "./dist/index/index.d.ts",
  "browser": {},
  "exports": {
    ".": {
      "development": {
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/dev.js"
        }
      },
      "import": {
        "types": "./dist/index/index.d.ts",
        "default": "./dist/index/index.js"
      }
    },
    "./plugins": {
      "development": {
        "import": {
          "types": "./dist/plugins/index.d.ts",
          "default": "./dist/plugins/dev.js"
        }
      },
      "import": {
        "types": "./dist/plugins/index.d.ts",
        "default": "./dist/plugins/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "plugins": [
        "./dist/plugins/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite serve dev",
    "build:dev": "vite build dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "dependencies": {
    "@babel/standalone": "^7.24.4",
    "@monaco-editor/loader": "^1.4.0",
    "@solid-primitives/resource": "^0.1.2",
    "@solid-primitives/scheduled": "^1.4.3",
    "clsx": "^2.1.0",
    "fflate": "^0.8.2",
    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "onigasm": "^2.2.5",
    "shiki": "^1.4.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.24.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/babel__standalone": "^7.1.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.0",
    "corvu": "^0.5.1",
    "esbuild": "^0.18.15",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.45.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.0",
    "tsup": "^7.1.0",
    "tsup-preset-solid": "^2.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-solid": "^2.7.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^0.33.0"
  },
  "keywords": [
    "solid",
    "repl",
    "playground",
    "typescript",
    "monaco"
  ],
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.0"
  }
}
